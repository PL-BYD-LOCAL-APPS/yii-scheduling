{
  "name": "anonymous-php/yii-scheduling",
  "description": "Scheduling extension for Yii 1.1.* framework",
  "keywords": [
    "yii",
    "scheduling",
    "cron"
  ],
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Anonymous PHP Developer",
      "email": "anonym.php@gmail.com"
    }
  ],
  "license": "MIT",
  "type": "yii-extension",
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii": "*",
    "yiisoft/mutex": "3.0.x-dev",
    "yiisoft/mutex-file": "3.0.x-dev",
    "symfony/process": "2.6.* || 3.* || 4.*",
    "mtdowling/cron-expression": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Required to use the thenPing method on schedules (~5.0)."
  },
  "autoload": {
    "psr-4": {
      "Anonymous\\Scheduling\\": "src"
    }
  },
  "extra": {
    "bootstrap": "Anonymous\\Scheduling\\Bootstrap"
  }
}
